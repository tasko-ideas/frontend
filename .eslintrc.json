{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": "off",
    "arrow-body-style": "off",
    "react/function-component-definition": "off",
    "react/prop-types": "off",
    "linebreak-style": "off"
  }
}
